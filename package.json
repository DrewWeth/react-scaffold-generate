{
  "name": "react-scaffold-generate",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "cli.js",
  "scripts": {
    "test": "jest",
    "start": "",
    "generate": "./cli.js generate model --path example-app/ date:string:date password:string:password color:string:color name:string description:string cost:number count:integer isTrue:boolean range:integer:range null:null updown:number:updown file:string:data-url email:string:email uri:string:uri datetime:string:date-time text:string:textarea",
    "watch": "npm-watch",
    "example-app-watch": "(cd example-app; npm run watch)",
    "example-app-run": "(cd example-app; npm start)"
  },
  "watch": {
    "example-app-run":{
      "patterns":["example-app/"]
    },
    "generate": {
      "clearBuffer": true,
      "patterns": [
        "cli.js",
        "templates"
      ],
      "extensions": ".js,.css",
      "ignore": "example-app/"
    }
  },
  "author": "Drew Wetherington (dgwetherington@gmail.com)",
  "license": "ISC",
  "dependencies": {
    "@rjsf/material-ui": "^2.4.1",
    "chalk": "^2.4.2",
    "figlet": "^1.5.0",
    "get-installed-path": "^4.0.8",
    "merge-json-file": "^1.0.1",
    "minimist": "^1.2.5",
    "react-notifications": "^1.7.2",
    "react-router-dom": "^5.2.0",
    "template-file": "^4.0.1"
  },
  "devDependencies": {
    "npm-watch": "^0.7.0"
  },
  "bin": {
    "react-scaffold-generate": "./cli.js"
  }
}
